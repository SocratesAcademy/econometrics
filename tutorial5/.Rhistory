set.seed(200)
df <- fabricate(
N = 200,
Y = rpois(N, lambda = 4),
Z = rbinom(N, 1, prob = 0.4),
X1 = Z * rbinom(N, 1, prob = 0.8),
X2 = rnorm(N),
G = sample(letters[1:4], N, replace = TRUE)
)
iv <- feols(Y ~ X2 | X1 ~ Z, data = df, se = 'hetero')
thef <- fitstat(iv, 'ivf', verbose = FALSE)$`ivf1::X1`$stat
